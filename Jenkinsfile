#!/usr/bin/env groovy
pipeline {
    agent any
    stages{
        stage('First stage'){
            steps{
                echo 'FS S1'
                echo 'FS S2'
                echo 'FS S3'
                echo 'FS S4'
            }
        }

        stage('Second stage'){
            steps{
                echo 'SS S1'
                echo 'SS S2'
                echo 'SS S3'
                echo 'SS S4'
            }
        }
    }
}